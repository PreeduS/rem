{
  "compilerOptions": {
    "strictNullChecks": true,
    //"lib": ["es6"]  //"lib": ["dom", "es2017", "esnext.asynciterable"],
    //"target": "es6",
    //"module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,

    "sourceMap": true,
    "allowJs": true,
    "outDir": "./build/",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,

    "baseUrl": ".",
    "paths": {
        "~/*": ["*"]
    }
  },
 // "include": [
 //   "./**/*"
 // ],
  "exclude": [
    "node_modules",
    "build"
  ],

}